        <div class="full-page register-page section-image" data-color="orange" data-image="../../assets/img/bg5.jpg">
            <div class="content">
                <div class="container">
                    <div class="card card-register card-plain text-center">
                        <div class="card-header ">
                            <div class="row  justify-content-center">
                                <div class="col-md-8">
                                    <div class="header-text">
                                        <h2 class="card-title">Investment Area</h2>
                                        <h4 class="card-subtitle">Please fill the questions so we can assist you accordingly</h4>
                                        <hr />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="container">
                            <div class="content">
                                <div class="card stacked-form">


                                    <div class="card-body ">
                                        <div class="row justify-content-center">


                                            <div class="col-md-10">
                                                @using (Html.BeginForm("AddAnswer", "Borrowers", FormMethod.Post))
                                                {
                                    @Html.HiddenFor(Model => Model.BorrowerQuestionID)
                                                    <div class="card card-plain">
                                                        <div class="content">
                                                            @foreach (var item in ViewBag.BorrowerQuestionslist)
                                                            {




                                            <p style="text-align:left">@item.Question </p>



                                                                <div class="form-group">
                                                                    @if (item.Control == "Text")
                                                                    {
                                                                        if (item.Question == "Current Types of Properties?")
                                                                        {

                                                @Html.DropDownList("Answers", (SelectList)ViewBag.ProductTypeID, "Seleccionar", new { @class = "form-control" }) }


                                            else if (item.Question == "Prior Type of properties?")
                                            {
                    @Html.DropDownList("Answers", (IEnumerable<SelectListItem>)ViewBag.ProductTypeID, "--- Select Type of Product ---", new { @class = "form-control form-control form-control-rounded" }) @*@Html.ValidationMessageFor(model => model.Answer, "", new { @class = "text-danger" })*@ }
                else if (item.Question == "Area of stock you like to buy?")
                {
@Html.DropDownList("Answers", (IEnumerable<SelectListItem>)ViewBag.AreaOfStocklist, "--- Select Area of Stock ---", new { @id = "aos", @class = "form-control form-control form-control-rounded" }) @*@Html.ValidationMessageFor(model => model.Answer, "", new { @class = "text-danger" })*@ }

else if (item.Question == "Comment")
{
@Html.TextBoxFor(model => model.Answers, new { @class = "form-control", placeholder = "Comment", @style = "height:100px" }) }
else
{
@Html.TextBoxFor(model => model.Answers, new { @class = "form-control" }) }
}
else if (item.Control == "Yes/No")
{
@Html.DropDownList("Answers", new List<SelectListItem>{new SelectListItem{ Text="Yes", Value="Yes"},new SelectListItem{ Text="No", Value="No"}

                                                                                       }, "--- Select Yes/No ---", new { @class = "form-control" }) }

else if (item.Control == "Multiple")
{

@Html.DropDownList("Answer", new SelectList(item.Investoroptionquestion, "OptionQuestion", "OptionQuestion")) }




else
{
@Html.TextBoxFor(model => model.Answers, new { htmlAttributes = new { @class = "form-control", placeholder = "Please Answer!", } })}

                                                                    @*@Html.ValidationMessageFor(model => model.Answer, "", new { @class = "text-danger db" })*@

                                                                </div>}
                                                        </div>
                                                        <div class="footer text-center">
                                                            @*<a href="@Url.Action("ProductsAccordingToAnswer", "Investor", new { area = "Investors" })" class="btn btn-fill btn-neutral btn-wd">Submit</a>*@

                                                            <button type="submit" class="btn btn-fill btn-neutral btn-wd">Submit</button>
                                                        </div>
                                                    </div>}

                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <nav>
